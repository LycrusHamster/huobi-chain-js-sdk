    "@types/jest": "^26.0.4",
    "graphql": "^14.6.0",
    "jest": "^26.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "trash-cli": "^3.0.0",
    "ts-jest": "^26.1.1",
    "typescript": "^3.9.6"
  }
